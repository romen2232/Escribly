{
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "tailwindConfig": "./frontend/tailwind.config.js",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-tailwindcss"]
}
